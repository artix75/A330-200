						<binding>
							<command>nasal</command>
							<script>
							 var gs = gui.Dialog.new("sim/gui/dialogs/gs/dialog", "Aircraft/A330-200/Systems/ground-services-dlg.xml");
							 gs.open();
							</script>
						</binding>
					</item>
					<item>
