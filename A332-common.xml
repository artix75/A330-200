							<command>nasal</command>
							<script>
							 var stair = gui.Dialog.new("sim/gui/dialogs/stairs/dialog", "Aircraft/A330-200/Systems/stairs.xml");
							 stair.open();
							</script>
						</binding>
					</item>
					<item>
